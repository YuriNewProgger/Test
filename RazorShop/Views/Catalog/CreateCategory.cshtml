@model dynamic

@{
    ViewBag.Title = "Create category";
    Layout = "_Layout";
}

<h2>Create new category</h2>

<form method="post">
    <p>
        <label>Id</label><br/>
        <input type="number" name="Id"/>    
    </p>
    
    <p>
        <label>Title</label><br/>
        <input type="text" name="Title"/>
    </p>
    
    <p>
        <input type="submit" value="Create"/>
    </p>
</form>
